<script>
document.addEventListener('DOMContentLoaded', function() {
  function initCountdownTimer() {
    const promoBar = document.querySelector('.promo-bar');
    if (!promoBar) {
      console.error('Element .promo-bar not found.');
      return;
    }

    // Получаем дату окончания из атрибута data-end
    let endDateStr = promoBar.dataset.end;

    if (!endDateStr) {
      console.error('data-end attribute is missing on .promo-bar element.');
      return;
    }

    // Предположим, что формат должен быть MM.DD.YYYY, но у нас "12.31..2024"
    // Уберём лишние точки: заменим все двойные точки на одну точку
    endDateStr = endDateStr.replace(/\.+/g, '.'); // "12.31..2024" -> "12.31.2024"

    const dateParts = endDateStr.split('.');
    if (dateParts.length !== 3) {
      console.error('Invalid date format. Expected MM.DD.YYYY');
      return;
    }

    const [monthStr, dayStr, yearStr] = dateParts;
    const month = parseInt(monthStr, 10);
    const day = parseInt(dayStr, 10);
    const year = parseInt(yearStr, 10);

    if (isNaN(month) || isNaN(day) || isNaN(year)) {
      console.error('Invalid date components.');
      return;
    }

    const endDate = new Date(year, month - 1, day);
    if (isNaN(endDate.getTime())) {
      console.error('Invalid date provided in data-end attribute.');
      return;
    }

    function updateCountdown() {
      const now = new Date();
      let timeRemaining = endDate.getTime() - now.getTime();

      if (timeRemaining <= 0) {
        timeRemaining = 0;
        clearInterval(intervalId);
        // Здесь можно добавить логику после завершения таймера
      }

      const totalSeconds = Math.floor(timeRemaining / 1000);
      const days = Math.floor(totalSeconds / (3600 * 24));
      const hours = Math.floor((totalSeconds % (3600 * 24)) / 3600);
      const minutes = Math.floor((totalSeconds % 3600) / 60);
      const seconds = totalSeconds % 60;

      const timeValues = [days, hours, minutes, seconds];
      const timeLabels = ['DAYS', 'HRS', 'MINS', 'SECS'];

      const counterItems = promoBar.querySelectorAll('.promo-bar__counter-item');
      counterItems.forEach((item, index) => {
        const valueSpan = item.querySelector('.value');
        const labelSpan = item.querySelector('.label');
        labelSpan.textContent = timeLabels[index];
        animateValueChange(valueSpan, timeValues[index]);
      });
    }

    function animateValueChange(element, newValue) {
      const currentValue = element.textContent;
      if (currentValue === newValue.toString()) {
        return;
      }
      element.classList.add('changing');
      setTimeout(() => {
        element.textContent = newValue;
        element.classList.remove('changing');
      }, 500);
    }

    updateCountdown();
    const intervalId = setInterval(updateCountdown, 1000);
  }

  initCountdownTimer();
});

document.addEventListener('DOMContentLoaded', () => {
    const promoBar = document.querySelector('.promo-bar');
    if (!promoBar) return;

    const stickyOffset = promoBar.offsetTop;

    window.addEventListener('scroll', () => {
        if (window.pageYOffset > stickyOffset) {
            promoBar.classList.add('fixed');
        } else {
            promoBar.classList.remove('fixed');
        }
    });
});
</script>

  




