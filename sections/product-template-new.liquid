                        <span>{{ a }}</span>
                    {% endfor %}

                {% endif %}
            </div>
            <h4 class="box__popup-content-title">VIDEOS FOR THIS PRODUCT</h4>
            <div class="box__popup-content-videos">
                {% for id in data.product_video_id_s.value %}
                    <div class="video-thumbnail">
                        <iframe
                                class="ytplayer"
                                width="350"
                                height="200"
                                src="https://www.youtube.com/embed/{{ id }}"
                                frameborder="0"
                        ></iframe>
                        <div class="video-overlay"></div>
                    </div>
                {% endfor %}
            </div>
            {% if data.product_for_related_products != blank %}
                {% assign related_product = data.product_for_related_products.value.metafields.custom.box_data.value %}
                <h4 class="box__popup-content-title">RELATED VIDEOS FOR THIS PRODUCT</h4>
                <div class="box__popup-content-videos">
                    <div class="video-thumbnail">
                        <iframe
                                class="ytplayer"
                                width="350"
                                height="200"
                                src="https://www.youtube.com/embed/{{ related_product.main_video_id }}"
                                frameborder="0"
                        ></iframe>
                        <div class="video-overlay"></div>
                    </div>
                    {% for id in related_product.product_video_id_s.value %}
                        <div class="video-thumbnail">
                            <iframe
                                    class="ytplayer"
                                    width="350"
                                    height="200"
                                    src="https://www.youtube.com/embed/{{ id }}"
                                    frameborder="0"
                            ></iframe>
                            <div class="video-overlay"></div>
                        </div>
                    {% endfor %}
                </div>
            {% endif %}
        </div>
    </div>
</div>
<script>
    document.addEventListener("DOMContentLoaded", () => {
        const popup = document.querySelector(".box__popup");
        const overlay = document.querySelector(".overlay");
        const openPopupBtn = document.querySelector(".box__content-btn");
        const closePopupBtn = document.querySelector(".box__popup-close");
        const videoThumbnails = document.querySelectorAll(".video-thumbnail");
        const mainVideo = document.querySelector(".box__popup-main-video iframe");
        const mainText = document.querySelector(".box__popup-main-copy > span"); // Целевой текстовый блок
        const descriptions = document.querySelectorAll(".box__popup-content-desc span"); // Тексты для подстановки
        const boxVideo = document.querySelector(".box__video iframe");
        const boxVideoDesc = document.querySelector('.box__content-copy_for_autoplay').textContent;

        if (!popup || !overlay || !openPopupBtn || !closePopupBtn || !mainVideo || !mainText || descriptions.length === 0) {
            console.error("Popup or required elements not found in the DOM.");
            return;
        }

        // Открытие попапа
        const showPopup = (event) => {
            mainVideo.src = boxVideo.src;
            mainText.innerHTML = boxVideoDesc;
            event.preventDefault();
            popup.classList.add("active");
            overlay.classList.add("active");
            document.querySelector('html').style.overflow = 'hidden';
            document.querySelector('.zoomContainer').style.zIndex = '-1';
            // Скроллим страницу вверх к попапу
            window.scrollTo({
                top: popup.offsetTop - 100, // Учитываем 100px сверху
                behavior: "smooth"
            });
        };

        // Закрытие попапа
        const closePopup = () => {
            popup.classList.remove("active");
            overlay.classList.remove("active");
            document.querySelector('html').style.overflow = 'auto';
            document.querySelector('.zoomContainer').style.zIndex = '1';
            mainVideo.src = "";

        };

        const changeMainVideo = (event) => {
            event.preventDefault();

            const clickedThumbnail = event.currentTarget;
            const iframe = clickedThumbnail.querySelector("iframe");
            const index = Array.from(videoThumbnails).indexOf(clickedThumbnail); // Получаем индекс кликнутого элемента

            if (!iframe) return;

            const videoID = new URL(iframe.src).pathname.split("/").pop();
            if (videoID) {
                mainVideo.src = `https://www.youtube.com/embed/${videoID}?autoplay=1`;
            }

            // Подставляем соответствующий текст
            if (descriptions[index]) {
                mainText.textContent = descriptions[index].textContent;
            }
        };

        // Навешиваем клик на `.video-thumbnail`, а не на iframe
        videoThumbnails.forEach((thumbnail) => {
            thumbnail.addEventListener("click", changeMainVideo);
        });

        // Слушатели событий
        openPopupBtn.addEventListener("click", showPopup);
        closePopupBtn.addEventListener("click", closePopup);
        overlay.addEventListener("click", closePopup);
    });
</script>
