                <iframe
                        class="ytplayer"
                        width="350"
                        height="200"
                        src="https://www.youtube.com/embed/{{ data.main_video_id }}"
                        frameborder="0"
                ></iframe>
            </div>
            {% if data.descriptions.value != blank %}
                <p class="box__content-copy box__popup-main-copy">
                    <svg width="25" height="25" viewBox="0 0 25 25" fill="none"
                         xmlns="http://www.w3.org/2000/svg"
                         xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect y="0.00146484" width="25" height="25"
                              fill="url(#pattern0_95_2730)"/>
                        <defs>
                            <pattern id="pattern0_95_2730"
                                     patternContentUnits="objectBoundingBox" width="1"
                                     height="1">
                                <use xlink:href="#image0_95_2730"
                                     transform="scale(0.00195312)"/>
                            </pattern>
                            <image id="image0_95_2730" width="512" height="512"
                                   preserveAspectRatio="none"
                                   xlink:href="data:image/png;base64,"/>
                        </defs>
                    </svg>
                    <span>{{ data.block_copy }}</span></p>
            {% endif %}
        </div>
        <div class="box__popup-content">
            <div class="box__popup-content-desc">
                {% for d in data.descriptions.value %}
                    <span>{{ d }}</span>
                {% endfor %}
                {% if data.product_for_related_products != blank %}
                    {% assign related_product_d = data.product_for_related_products.value.metafields.custom.box_data.value %}
